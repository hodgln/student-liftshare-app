{
  "expo": {
    "name": "Spareseat",
    "slug": "Spareseat",
    "scheme": "spareseat",
    "version": "1.0.9",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "platforms": [
      "ios",
      "android"
    ],
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.danhodgin.Spareseat"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0466C8"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
